{% extends 'base.html.twig' %}

{% block title %}Verify OTP{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('auth-verify') }}
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('auth-verify') }}
{% endblock %}

{% block body %}
<div
    id="verify-root"
    data-error="{{ error|default('') }}"
    data-phone-number="{{ phone_number }}"
    data-expires-in="{{ expires_in }}"
></div>
{% endblock %}
