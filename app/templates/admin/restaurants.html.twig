{% extends 'base.html.twig' %}

{% block title %}Admin - Restaurants{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('admin-restaurants') }}
{% endblock %}

{% block body %}
    <div id="admin-restaurants-root"
         data-restaurants="{{ restaurants|e('html_attr') }}"
         data-current-page="{{ currentPage }}"
         data-total-pages="{{ totalPages }}"
         data-search="{{ search }}"
         data-total="{{ total }}"></div>
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('admin-restaurants') }}
{% endblock %}
