{% extends 'base.html.twig' %}

{% block title %}Admin - Utilisateurs{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('admin-users') }}
{% endblock %}

{% block body %}
    <div id="admin-users-root"
         data-users="{{ users|e('html_attr') }}"
         data-restaurants="{{ restaurants|e('html_attr') }}"
         data-current-page="{{ currentPage }}"
         data-total-pages="{{ totalPages }}"
         data-search="{{ search }}"
         data-total="{{ total }}"></div>
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('admin-users') }}
{% endblock %}
